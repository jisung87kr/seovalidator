APP_NAME=SEOValidator
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

# Sanctum Configuration
SANCTUM_STATEFUL_DOMAINS=localhost,localhost:3000,127.0.0.1,127.0.0.1:8000,::1
SANCTUM_TOKEN_PREFIX=seo_

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database Configuration for Docker
DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=seovalidator
DB_USERNAME=laravel
DB_PASSWORD=laravel_password

SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis

CACHE_STORE=redis
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

# Redis Configuration for Docker
REDIS_CLIENT=phpredis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# External API Configuration
GOOGLE_PAGESPEED_API_KEY=
GOOGLE_PAGESPEED_MAX_RETRIES=3
GOOGLE_PAGESPEED_RETRY_DELAY=1000
GOOGLE_PAGESPEED_CACHE_TIMEOUT=3600

MOZ_ACCESS_ID=
MOZ_SECRET_KEY=
MOZ_MAX_RETRIES=3
MOZ_RETRY_DELAY=1000
MOZ_CACHE_TIMEOUT=3600

# Sentry Configuration
SENTRY_LARAVEL_DSN=
SENTRY_RELEASE=
SENTRY_ENVIRONMENT=local
SENTRY_SAMPLE_RATE=1.0
SENTRY_TRACES_SAMPLE_RATE=0.1
SENTRY_PROFILES_SAMPLE_RATE=
SENTRY_SEND_DEFAULT_PII=false

# Telescope Configuration
TELESCOPE_ENABLED=true
TELESCOPE_DOMAIN=
TELESCOPE_PATH=telescope
TELESCOPE_DRIVER=database
TELESCOPE_QUEUE_CONNECTION=
TELESCOPE_QUEUE=
TELESCOPE_QUEUE_DELAY=10
TELESCOPE_SLOW_QUERY_THRESHOLD=250

# Performance Optimization
CACHE_OPTIMIZATION_ENABLED=true
DATABASE_OPTIMIZATION_ENABLED=true
PERFORMANCE_MONITORING_ENABLED=true

# Load Testing Configuration
LOAD_TEST_ENABLED=false
LOAD_TEST_MAX_CONCURRENT_USERS=100
LOAD_TEST_DEFAULT_DURATION=300

# External Service Timeouts
HTTP_CLIENT_TIMEOUT=30
HTTP_CLIENT_CONNECT_TIMEOUT=10
HTTP_CLIENT_MAX_REDIRECTS=3

# Rate Limiting
API_RATE_LIMIT_PER_MINUTE=60
ANALYSIS_RATE_LIMIT_PER_HOUR=100
